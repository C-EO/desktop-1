{
  "name": "gatsby-desktop",
  "version": "0.0.1",
  "main": "lib/main.js",
  "author": "<matt@gatsbyjs.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.10.5",
    "core-js": "^3.6.5",
    "electron": "^9.1.2",
    "execa": "^4.0.3",
    "fs-extra": "^9.0.1",
    "gatsby": "^2.24.13",
    "gatsby-interface": "^0.0.182",
    "menubar": "^9.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.1",
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.43",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.5.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "microbundle": "^0.12.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "typescript": "^3.9.7"
  },
  "scripts": {
    "build:renderer": "gatsby build",
    "watch:renderer": "gatsby develop --port=8079",
    "watch:main": "microbundle -i app/main.ts --target=node -o lib/main.js --no-pkg-main --tsconfig=app/tsconfig.json -f cjs watch",
    "build:main": "microbundle -i app/main.ts --target=node -o lib/main.js --no-pkg-main --tsconfig=app/tsconfig.json -f cjs build",
    "build:worker": "microbundle -i worker/launcher.ts -o lib/launcher.js --no-pkg-main --tsconfig=worker/tsconfig.json -f cjs --compress=false --external=fs,constants,stream,child_process,os build",
    "watch:worker": "microbundle -i worker/launcher.ts -o lib/launcher.js --no-pkg-main --tsconfig=worker/tsconfig.json -f cjs --compress=false --external=fs,constants,stream,child_process,os watch",
    "run:app": "electron --remote-debugging-port=8315 .",
    "watch:app": "GATSBY_DEVELOP_URL=http://localhost:8079 yarn run:app",
    "develop": "FORCE_COLOR=1 npm-run-all -l build:main --parallel watch:*"
  }
}
